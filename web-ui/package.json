    "react": "16.13.1",
    "react-dom": "16.13.1",
    "style-loader": "^0.23.1",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "4.1.3",
    "mocha": "6.2.0",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "@types/chai": "4.1.7",
    "webpack": "5.33.2",
    "webpack-cli": "4.6.0",
    "copy-webpack-plugin": "^5.0.3",
